{
  "manifest_version": 3,
  "name": "ArticleBin",
  "version": "1.0",
  "description": "Extract the articles from your website.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["activeTab", "tabs"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
